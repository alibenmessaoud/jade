package it.unitn.science.aose.salnitri.posWeatherForecast;

import jade.core.AID;

/**
 * this class contains information about a WFS and the price it offered.
 * 
 * @author Mattia Salnitri
 *
 */
public class WFServiceDescriptor 
{
	private AID service;
	private int price;
	
	public WFServiceDescriptor(AID service, int price)
	{
		this.price=price;
		this.service=service;
	}

	public AID getService() {
		return service;
	}

	public void setService(AID service) {
		this.service = service;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

}
